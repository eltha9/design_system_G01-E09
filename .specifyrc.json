{
  "repository": "@hetic/G01_E09_LightTheme",
  "base": "master",
  "head": "specify",
  "rules": [{
      "path": "sass/01-utils/variables/colors.scss",
	  "filter": {
		"types": ["color"]
	  },
      "parsers": [{
          "name": "camelcasify",
          "options": {
            "keys": ["name"]
          }
        },
        {
          "name": "to-css-custom-properties"
        }
      ]
    },
    {
      "path": "sass/01-utils/variables/mesurement.scss",
	  "filter": {
		"types": ["mesurement"]
	  },
      "parsers": [{
          "name": "camelcasify",
          "options": {
