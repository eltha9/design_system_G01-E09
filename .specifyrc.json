	  "filter": {
		"types": ["shadow"]
	  },
      "parsers": [{
          "name": "camelcasify",
          "options": {
            "keys": ["name"]
          }
        },
        {
          "name": "to-css-custom-properties"
        }
      ]
    },
    {
      "path": "sass/01-utils/variables/border.scss",
	  "filter": {
		"types": ["border"]
